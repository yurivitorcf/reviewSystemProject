<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rating_Must_Be_Between_1_and_5</fullName>
    <active>true</active>
    <errorConditionFormula>OR(
   Rating__c &lt; 1,
   Rating__c &gt; 5
)</errorConditionFormula>
    <errorMessage>A avaliação precisa ser entre 1 e 5 estrelas.</errorMessage>
</ValidationRule>
